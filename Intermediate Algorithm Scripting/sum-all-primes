function sumPrimes(num) {
  //console.log(isPrime(10));
var resultado = 0;

for(let i=2;i<=num;i++)
{
  if (isPrime(i))
    resultado += i;
}
  return resultado;
}

function isPrime(number)
{
  for (let i=2;i<number;i++){
    if (number % i == 0)
      return false;
  }
  return true;
}


console.log(sumPrimes(977));
